def_key "+"
    show_clock
def_key "="
    volume_up

def_key "j"
    scroll_down
def_key "k"
    scroll_up

def_key "ctrl-u"
    page_up
#push_characters "kkkkkkkkkkkkkkk"
def_key "ctrl-d"
    page_down
#push_characters "jjjjjjjjjjjjjjj"

def_key "h"
    previous_column
def_key "h"
    jump_to_parent_directory
def_key "h"
    replay_song

def_key "l"
    next_column
def_key "l"
    enter_directory
def_key "l"
    play_item

def_key "."
    show_lyrics

def_key "n"
    next_found_item
def_key "N"
    previous_found_item

def_key "ctrl-j"
    move_sort_order_down
def_key "J"
    move_sort_order_down
def_key "ctrl-k"
    move_sort_order_up
def_key "K"
    move_sort_order_up
def_key "ctrl-j"
    move_selected_items_down
def_key "J"
    move_selected_items_down
def_key "ctrl-k"
    move_selected_items_up
def_key "K"
    move_selected_items_up

def_key ","
  move_sort_order_down
def_key ","
  move_selected_items_down
